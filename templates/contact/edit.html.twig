{% extends 'back.html.twig' %}


{% block title %}Edit Contact{% endblock %}


{% block body %}
  <div class="formulaire_edit">
    
   <h1>Edit Contact</h1>


   {{ form_start(form) }}
       {{ form_row(form.nom) }}
       {{ form_row(form.prenom) }}
       {{ form_row(form.email) }}
       {{ form_row(form.message) }}
       <button type="submit" class="btn btn-primary">Enregistrer</button>
   {{ form_end(form) }}


</div>

{% endblock %}
